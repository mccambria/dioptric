    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug console",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole",
            "redirectOutput": true,
            "suppressMultipleSessionWarning": true,
            "justMyCode": false,
        },
        {
            "name": "Integrated terminal",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "redirectOutput": true,
            "suppressMultipleSessionWarning": true,
            "justMyCode": false,
        }
    ]
}